<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.richard.intent_intro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".usernameActivity" />
        <activity android:name=".LayoutActivity" />
        <activity android:name=".lifecycleActivity" />
        <activity android:name=".un_layoutActivity" />
        <activity
            android:name=".un_DialogActivity"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".button" />
        <activity android:name=".buttontoast"></activity>
    </application>

</manifest>